{
  "name": "duncrow-gmbh/flipbert",
  "version": "2.0.0",
  "description": "Flipbert Extension for Contao Open Source CMS",
  "keywords": [
    "contao",
    "flipbert",
    "flipbook",
    "pdf"
  ],
  "type": "contao-bundle",
  "homepage": "https://flipbert.at",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Duncrow GmbH",
      "email": "office@duncrow.com",
      "homepage": "https://duncrow.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "contao/core-bundle": "^5.3",
    "guzzlehttp/guzzle": "^6.5 || ^7.4"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "DuncrowGmbh\\Flipbert\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "files": [],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "DuncrowGmbh\\Flipbert\\ContaoManager\\Plugin"
  }
}
